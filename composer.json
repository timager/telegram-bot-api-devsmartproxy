{
    "name": "devsmart/telegram-bot",
    "description": "Telegram Bot API with devsmartproxy",
    "homepage": "https://github.com/timager/telegram-bot-api-devsmartproxy",
    "license": "MIT",
    "require": {
        "php" : ">=5.5.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0",
        "squizlabs/php_codesniffer": "2.*",
        "codeception/codeception": "*"
    },
    "autoload": {
        "psr-4": {
            "TelegramBot\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TelegramBot\\Api\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
